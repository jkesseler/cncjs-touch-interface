{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "8",
        "browsers": ["last 2 versions"]
      }
    }]
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@constants": "./src/constants",
        "@assets": "./src/assets",
        "@ui": "./src/app/components",
        "@pages": "./src/app/pages",
        "@utils": "./src/app/utils",
        "@state": "./src/app/state"
      }
    }]
  ]
}
